{
	"name": "@koota/debug",
	"version": "0.0.1",
	"description": "Debug tools for Koota",
	"private": true,
	"license": "ISC",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./plugin": {

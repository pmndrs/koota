		"magic-string": "^0.30.17",
		"react-force-graph-2d": "^1.29.0",
		"unplugin": "^2.3.11"
	},
	"devDependencies": {
		"@config/oxlint": "workspace:*",
		"@config/typescript": "workspace:*",
		"@types/estree": "^1.0.7",
		"@types/react": "catalog:",
		"react": "catalog:",
		"tsup": "^8.3.0",
		"vitest": "catalog:"
	},
	"peerDependencies": {
		"react": ">=18"
	}
